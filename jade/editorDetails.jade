mixin header(projectTitle, projectType)
  div.jumbotron(style={ margin:' 0 -0.9375rem'} )
    div.backgroundSpecial
      div.content
        h1 #{projectTitle}
        h4 #{projectType}
        
mixin generalInfo()
  div.cardSpecial(style={'margin-top':'5%', 'margin-bottom': '5%' ,'box-shadow': '0px 1px 5px #888888', 'background-color': 'none !important'})
    div.nav-tabs-horizontal.nav-tabs-inverse(data-plugin='tabs')
      ul.nav.nav-tabs.nav-tabs-solid(role='tablist')
        li.nav-item(role='presentation')
          a.nav-link.active(data-toggle='tab', href='#exampleIconifiedTabsOne', aria-controls='exampleIconifiedTabsOne', role='tab', aria-expanded='true')
            i.icon.md-home(aria-hidden='true')
            |  General
        li.nav-item(role='presentation')
          a.nav-link(data-toggle='tab', href='#exampleIconifiedTabsTwo', aria-controls='exampleIconifiedTabsTwo', role='tab', aria-expanded='false')
            i.icon.md-account-circle(aria-hidden='true')

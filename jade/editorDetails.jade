        li.nav-item(role='presentation')
          a.nav-link(data-toggle='tab', href='#exampleIconifiedTabsFour', aria-controls='exampleIconifiedTabsFour', role='tab', aria-expanded='false')
            i.icon.md-accounts(aria-hidden='true')
            |  Miembros
      div.tab-content.pt-15
        div#exampleIconifiedTabsOne.tab-pane.active(role='tabpanel', aria-expanded='true')
          div.row
            div.col-xs-12.col-md-12
              h1 Datos Generales
            div.col-md-5.col-xs-12.offset-md-1.form-group(style={'margin-top': '3%'})
              p
                b Tipo de Proyecto: 
                input.form-control(type="text")
              p
                b Cliente/Empresa: 
                input.form-control(type="text")
              p
                b Giro de la Empresa:
                input.form-control(type="text")
              p
                b Lugar de Realización:
                input.form-control(type="text")
              p
                b Fecha Inicio:
                div.input-group(style={'margin-bottom': '1%'})

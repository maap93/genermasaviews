doctype html
html(lang='es-mx')
  head
    meta(charset='utf-8')
    meta(http-equiv='Content-Type' content='text/html; charset=utf-8')
    meta(http-equiv='X-UA-Compatible' content='ie=edge,chrome=1')
    
    block title
      title Genermasa
    //- SEO indexing & responsiveness
    link(rel='canonical' href=(self.url))
    meta(name='viewport' content='width=device-width, initial-scale=1.0, minimum-scale=1.0')
    meta(name='referrer' content='no-referrer-when-downgrade')
    meta(name='robots' content='index, follow, noimageindex')
    
    //- Site details
    meta(name='author' content='Asora')
    meta(name='description' content=(self.description))
    meta(property='og:type' content='website')
    //- meta(property='og:title' content=(self.social.title))
    //- meta(property='og:image' content=(self.social.image))
    //- meta(property='og:url' content=(self.social.url))
    //- meta(property='og:description' content=(self.social.description))
    
    //- Stylesheets & scripts
    block stylesheets
      link(rel="stylesheet" href="global/css/bootstrap.min.css")
      link(rel="stylesheet" href="global/css/bootstrap-extend.min.css")
      link(rel="stylesheet" href="assets/css/site.min.css")
      link(rel="stylesheet" href="global/vendor/animsition/animsition.css")
      link(rel="stylesheet" href="global/vendor/asscrollable/asScrollable.css")
      link(rel="stylesheet" href="global/vendor/switchery/switchery.css")
      link(rel="stylesheet" href="global/vendor/slidepanel/slidePanel.css")
      link(rel="stylesheet" href="global/vendor/flag-icon-css/flag-icon.css")
      link(rel="stylesheet" href="global/fonts/material-design/material-design.css")
    
    block scriptsHead
      script(src="global/vendor/breakpoints/breakpoints.js")
      script(src="global/vendor/jquery/jquery.js")
      script(src='https://code.jquery.com/jquery-1.12.4.js')
      script(src='https://code.jquery.com/ui/1.12.1/jquery-ui.js')


    block icons
      link(rel="stylesheet" href="global/fonts/web-icons/web-icons.min.css")
      link(rel="stylesheet" href="global/fonts/brand-icons/brand-icons.min.css")
    
    block fontStyles
      link(rel='stylesheet' href='http://fonts.googleapis.com/css?family=Roboto:300,400,500,300italic')
    
    block additional_styles
    
  body(style={'background-color': '#F1F4F5'})
    
    // Content Section
    block content
    
    // Neccesary scripts section
    block scriptBottom
      script(src="global/vendor/babel-external-helpers/babel-external-helpers.js")
      script(src="global/vendor/tether/tether.js")
      script(src="global/vendor/bootstrap/bootstrap.js")
      script(src="global/vendor/animsition/animsition.js")
      script(src="global/vendor/mousewheel/jquery.mousewheel.js")
      script(src="global/vendor/asscrollbar/jquery-asScrollbar.js")
      script(src="global/vendor/asscrollable/jquery-asScrollable.js")
      script(src="global/vendor/ashoverscroll/jquery-asHoverScroll.js")
      script(src="global/vendor/screenfull/screenfull.js")
      script(src="global/vendor/slidepanel/jquery-slidePanel.js")
      script(src="global/js/State.js")
      script(src="global/js/Component.js")
      script(src="global/js/Plugin.js")
      script(src="global/js/Base.js")
      script(src="global/js/Config.js")
      script(src="assets/js/Section/Menubar.js")
      script(src="assets/js/Section/GridMenu.js")
      script(src="assets/js/Section/Sidebar.js")
      script(src="assets/js/Section/PageAside.js")
      script(src="assets/js/Plugin/menu.js")
      script(src='global/js/config/colors.js')
      script.
        Config.set('assets', 'assets');

    
    block additional_scripts
      
    
    
    
    
    
    
    
    
    
    
    
    
    
    

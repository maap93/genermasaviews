mixin modalExpenses()
  div#myModal.modal.fade(tabindex='-1', role='dialog', aria-labelledby='exampleModalLabel', aria-hidden='true', style={'z-index': '20001'})
    div.modal-dialog(role='document', style={'width' : '800px !important', 'max-width' : '800px !important' })
      div.modal-content
        div.modal-header
          h5#exampleModalLabel.modal-title Registrar Gasto
          button.close(type='button', data-dismiss='modal', aria-label='Close')
            span(aria-hidden='true') ×
        div.modal-body
          form(action="#" method="method")
            div.row.card-block
              div.col-xs-12.col-md-12
              div.col-md-12.col-xs-12
                div.form-group
                  label(for='conceptoGeneral') Concepto General
                  select.form-control(style={'margin-bottom': '1%'})
                    option(value="UV") Unidad de Verificación (Auditorias) 
                    option(value="PA") Proyectos Ambientales
                    option(value="PE") Proyectos Especiales
                    option(value="SU") Suelo
                    option(value="IN") Inogen
                    option(value="OVV") Ovvalo
                div.form-group
                  label(for='conceptoEspecifico') Concepto Especifico
                  select.form-control(style={'margin-bottom': '1%'})
                    option(value="UV") Unidad de Verificación (Auditorias) 
                    option(value="PA") Proyectos Ambientales
                    option(value="PE") Proyectos Especiales
                    option(value="SU") Suelo
                    option(value="IN") Inogen
                    option(value="OVV") Ovvalo
                div.form-group
                  label(for='detalle') Detalle
                  input.form-control(type='text', name='detalle' placeholder='')
                div.form-group
                  label(for='importe') Importe
                  input.form-control(type='number', name='importe' placeholder='$ 525.00')
                div.form-group
                  label(for='iva') I.V.A
                  input.form-control(type='number', name='iva' placeholder='$ 25.00')
        div.modal-footer
          button.btn.btn-secondary(type='button', data-dismiss='modal') Cerrar
          button.btn.btn-primary(type='button') Guardar Cambios

      
block content
  include ./navbar.jade
  div.container-fluid
    +header("Proyecto de Ovvalo", "Ovvalo")
    div.row
      div.col-md-10.offset-md-1.col-xs-12
        +generalInfo()
        +graphNumbers()
    +modalExpenses()
